{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C_Exercises/GOODTASKS.JSON",
			"command": "/bin/gcc-9",
			"args": [
				"-fdiagnostics-color=always",
				"-Wall",
				"-Wextra",
				"-Wpedantic",
				"-Waggregate-return",
				"-Wwrite-strings",
				"-Wvla",
				"-Wfloat-equal",
				"-pthread",
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /bin/gcc-9"
		}
	]
}
